<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>crazyswarm_application</name>
  <version>0.0.0</version>
  <description>ROS2 Package for Crazyswarm Application</description>
  <maintainer email="matthewoots@gmail.com">Matthew Woo</maintainer>
  <license>TODO</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>rosidl_default_generators</buildtool_depend>

  <build_depend>rclcpp</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>tf2_ros</build_depend>
  <build_depend>builtin_interfaces</build_depend>
  <build_depend>rosgraph_msgs</build_depend>
  <build_depend>rviz_2d_overlay_msgs</build_depend>
  <build_depend>GTSAM</build_depend>

  <build_depend>crazyflie_interfaces</build_depend>
  <build_depend>apriltag_msgs</build_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <exec_depend>rosidl_default_runtime</exec_depend>

  <member_of_group>rosidl_interface_packages</member_of_group>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
